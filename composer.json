{
	"name": "fuzz/magic-box",
	"description": "A magical implementation of Laravel's Eloquent models as injectable, masked resource respositories.",
	"require": {
		"illuminate/database": "5.1.x",
		"fuzz/laravel-data": "3.0.x-dev"
	},
	"require-dev": {
		"orchestra/testbench": "3.1.*"
	},
	"authors": [
		{
			"name": "Fuzz Productions",
			"email": "fuzzweb@fuzzproductions.com"
		}
	],
	"repositories": {
		"laravel-data": {
			"type": "vcs",
			"url": "git@gitlab.fuzzhq.com:web-modules/laravel-data.git"
		}
	},
	"autoload": {
		"psr-4": {
			"Fuzz\\MagicBox\\": "src/",
			"Fuzz\\MagicBox\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev"
}
